{
  "name": "clapperboard",
  "version": "1.0.4",
  "description": "Easily set frames to create an animation rendered from an object (like the state of a react component).",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/joedelia/clapperboard.git"
  },
  "keywords": [
    "Animation"
  ],
  "author": "Joe d'Elia",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/joedelia/clapperboard/issues"
  },
  "homepage": "https://github.com/joedelia/clapperboard#readme",
  "dependencies": {
    "dot-object": ">=2.1.3",
    "lodash": "^4.17.21"
  },
  "xo": {
    "envs": [
      "browser"
    ],
    "rules": {
      "no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "args": "none",
          "varsIgnorePattern": "^_"
        }
      ],
      "one-var": [
        "warn",
        {
          "uninitialized": "always"
        }
      ],
      "curly": [
        "error",
        "multi-or-nest",
        "consistent"
      ],
      "no-new": 0,
      "no-prototype-builtins": 0,
      "no-loop-func": 0,
      "no-alert": 0,
      "indent": [
        "error",
        2
      ]
    },
    "ignores": [
      "Gruntfile.js"
    ]
  },
  "devDependencies": {
    "xo": "^0.17.1"
  }
}
